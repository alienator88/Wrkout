        <activity
            android:name=".day1_ab"
            android:label="@string/title_activity_day1_ab"
            android:theme="@style/miniTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>